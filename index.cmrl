<?xml version="1.0" encoding="UTF-8"?>
<cmrl xmlns:dotgo="http://dotgo.com/cmrl/1.0">
	
	<match pattern="twitter">
		<engine href="http://www.openqry.org/twitter/index.php" />
	</match>
	<match pattern="tumblr">
		<engine href="http://www.openqry.org/tumblr/tumblr.php" />
	</match>
	<match pattern="news">
		<engine href="http://www.openqry.org/nyt/index.php" />
	</match>
	<match pattern="">
		<message>
	      <content>reply <anchor query="h response"><engine href="http://www.openqry.org/nyt/index.php" /></anchor> + topic for news, <anchor><engine href="http://www.openqry.org/twitter/index.php" /></anchor> + topic for twitter, or <anchor><engine href="http://www.openqry.org/tumblr/tumblr.php" /></anchor>  + topic for tumblr to continue!</content>
	    </message>
	  </match>
	<match pattern="*">
		<message>
			<content>reply <anchor><engine href="http://www.openqry.org/nyt/index.php" /></anchor> + topic for news, <anchor><engine href="http://www.openqry.org/twitter/index.php" /></anchor> + topic for twitter, or <anchor><engine href="http://www.openqry.org/tumblr/tumblr.php" /></anchor>  + topic for tumblr to continue!</content>
		</message>
	</match>
</cmrl>

