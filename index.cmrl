<?xml version="1.0" encoding="UTF-8"?>
<cmrl xmlns:dotgo="http://dotgo.com/cmrl/1.0">
	
	<match pattern="twitter">
		<engine href="http://www.openqry.com/twitter/index.php" />
	</match>
	<match pattern="tumblr">
		<engine href="http://www.openqry.com/tumblr/tumblr.php" />
	</match>
	<match pattern="news">
		<engine href="http://www.openqry.com/nyt/index.php" />
	</match>
	<match pattern="">
	    <message>
		<message>
	      <content>text <anchor><engine href="http://www.openqry.com/nyt/index.php" />"news"</anchor>, <anchor><engine href="http://www.openqry.com/twitter/index.php" />"twitter"</anchor>, or <anchor><engine href="http://www.openqry.com/tumblr/tumblr.php" />"tumblr"</anchor> to continue!</content>
	    </message>
	  </match>
	<match pattern="*">
		<message>
			<content>text <anchor><engine href="http://www.openqry.com/nyt/index.php" />"news"</anchor>, <anchor><engine href="http://www.openqry.com/twitter/index.php" />"twitter"</anchor>, or <anchor><engine href="http://www.openqry.com/tumblr/tumblr.php" />"tumblr"</anchor> to continue!?</content>
		</message>
	</match>
</cmrl>

